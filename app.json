{
  "expo": {
    "name": "Eliav's Calculator",
    "slug": "ESCalculator",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "android": {
      "package": "com.eliavco.escalculator"
    },
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/images/icon-white.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.eliavco.escalculator"
    },
    "description": "This is a simple and functional calculator made for everyone with <3 By Eliav Cohen!",
    "githubUrl": "https://github.com/eliavco/react-native-app-one.git"
  }
}
